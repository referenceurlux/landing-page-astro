---
import { WordpressServices } from "../../data/wordpress";
import About from "./About.astro";
import About3 from "./About3.astro";
import About5 from "./About5.astro";
interface Props {
  slug: string;
}
const {slug} = Astro.props;
const page = await WordpressServices.getPageBySlug(slug);
const template = page.acf.template;
---

{template === "Template 1" || template === "Template 2" ? <About slug={page.slug} /> : null}
{template === "Template 3" || template === "Template 4" ? <About3 slug={page.slug} /> : null}
{template === "Template 5" ? <About5 slug={page.slug} /> : null}