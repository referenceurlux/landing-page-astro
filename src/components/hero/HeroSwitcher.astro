---
import { WordpressServices } from "../../data/wordpress";
import Hero from "./Hero.astro";
import Hero3 from "./Hero3.astro";
interface Props {
  slug: string;
}
const {slug} = Astro.props;
const page = await WordpressServices.getPageBySlug(slug);
const template = page.acf.template;
---

{template === "Template 1" ? <Hero slug={page.slug} /> : null}
{template === "Template 3" ? <Hero3 slug={page.slug} /> : null}