---
import { WordpressServices } from "../../data/wordpress";
import Contact from "./Contact.astro";
import Contact3 from "./Contact3.astro";
interface Props {
  slug: string;
}
const {slug} = Astro.props;
const page = await WordpressServices.getPageBySlug(slug);
const template = page.acf.template;
---

<!-- {template === "Template 1" ? <Contact slug={page.slug} /> : null} -->
{template === "Template 3" ? <Contact3 slug={page.slug} /> : null}